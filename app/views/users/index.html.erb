<div class="container">
  <div class="row">
    <div class="col-md-3">
      <%= render 'users/user', user: @user %>
      <h2>New book</h2>
      <%= render 'books/book', book: @book %>
    </div>



    <div class='col-md-8 offset-md-1'>
      <h2>Users</h2>
      <table class='table'>
  <thead>
      <th>image</th>
      <th>name</th>
  </thead>
        <tbody>
  	  	  <% @users.each do |user| %>
	  	  	<tr>
	  	  	  <td><%= image_tag user.get_profile_image(100,100) %></td>
	  	  	  <td><%= user.name %></td>
	  	  	  <td><%= link_to "Show", user_path(user.id) %></td>
	  	  	</tr>
  	  	  <% end %>
  	  	</tbody>
  	  	</table>
  	</div>
  </div>
